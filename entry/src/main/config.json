{
  "app": {
    "bundleName": "com.example.learnmyapplication_ark",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {
    "default": {
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.NoTitleBar"
        }
      ]
    },
    "package": "com.example.learnmyapplication_ark",
    "name": ".MyApplication",
    "mainAbility": "com.example.learnmyapplication_ark.MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home",
              "ability.intent.QUERY_WEATHER",
              "ability.intent.DEFAULT_ACE",
              "action.qr.scanner"
            ]
          }
        ],
        "visible": true,
        "name": "com.example.learnmyapplication_ark.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard",
        "orientation": "portrait"
      },
      {
        "orientation": "unspecified",
        "name": "com.example.learnmyapplication_ark.ScanningQRAbility",
        "icon": "$media:icon",
        "description": "$string:scanningqrability_description",
        "label": "$string:entry_ScanningQRAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "backgroundModes": [
          "dataTransfer"
        ],
        "name": "com.example.learnmyapplication_ark.QrScanningServiceAbility",
        "icon": "$media:icon",
        "description": "$string:qrscanningserviceability_description",
        "type": "service"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.CAMERA"
      },
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "name": "ohos.permission.READ_USER_STORAGE"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index",
          "pages/first_page/first_page",
          "pages/formDesigner/formDesigner",
          "pages/drawer/drawer",
          "pages/FComponent/FComponent",
          "pages/FComponent/checkBoxWidget/checkBoxWidget",
          "pages/taskList/taskList",
          "pages/userOpPage/userOpPage",
          "pages/FComponent/ImageWidegt/ImageWidegt"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      },
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "card",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      },
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "Scanning",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      }
    ]
  }
}